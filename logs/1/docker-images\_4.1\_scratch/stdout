/bin/bash /tmp/vorbis/install-sh -d /opt/ffmpeg/share/doc/libvorbis-1.3.6
if test -d vorbis; then \
  for dir in vorbis/*; do \
    if test -d $dir; then \
      b=`basename $dir`; \
      /bin/bash /tmp/vorbis/install-sh -d /opt/ffmpeg/share/doc/libvorbis-1.3.6/$b; \
      for f in $dir/*; do \
	/usr/bin/install -c -m 644 $f /opt/ffmpeg/share/doc/libvorbis-1.3.6/$b; \
      done \
    fi \
  done \
fi
